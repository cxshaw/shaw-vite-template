{
  "name": "uniubi-construction-wifi-education",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint '**/*.{js,jsx,tsx,ts}'",
    "lint:fix": "eslint '**/*.{js,jsx,tsx,ts}' --fix",
    "prettier": "prettier --write '**/*.{less,css,md,json}'"
  },
  "dependencies": {
    "antd": "^4.19.5",
    "axios": "^0.26.1",
    "classnames": "^2.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "@vitejs/plugin-react": "^1.0.7",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "consola": "^2.15.3",
    "dotenv": "^16.0.0",
    "eslint": "^8.12.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.1",
    "shaw-lint": "^1.0.0",
    "typescript": "^4.5.4",
    "vite": "^2.9.0",
    "vite-plugin-style-import": "^2.0.0",
    "vite-react-jsx": "^1.1.2",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint:fix",
      "git add ."
    ],
    "*.{less,css,md,json}": [
      "npm run prettier",
      "git add ."
    ]
  },
  "license": "MIT"
}
